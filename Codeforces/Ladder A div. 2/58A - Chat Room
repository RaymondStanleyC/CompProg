// 58A - Chat room

#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

vector<int> arr;

int main(){

	string s;
	string h= "hello";

	cin >> s;
	int j=0;
	for(int i=0; i<s.size(); i++){
		if(s[i] == h[j]){
			j++;
		}
		if(j == h.size()){
			cout << "YES";
			return 0;
		}
	}

	cout << "NO";
	return 0;

}
