// 233A - Perfect Permutation

#include<iostream>
#include <vector>
#include <algorithm>
#include <set>
#include <cstring>
#include <map>
#include <list>

using namespace std;

char matrix[309][309];

int main(){
    
    int n;
    
    cin >>n;
    if(n%2 == 1) cout << -1;
    
    else{
        for(int i=1; i<=n; i+=2){
            cout << i+1 << " " << i << " ";
        }
    }
    
    
    return 0;
}