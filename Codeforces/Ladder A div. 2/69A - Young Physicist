// 69A - Young Physicist

#include <iostream>

using namespace std;

int main(){

	int x = 0;
	int y = 0;
	int z = 0;
	int n;
	int temp;

	cin >> n;

	for(int i=0; i<n; i++){
		cin >> temp;
		x += temp;
		cin >> temp;
		y += temp;
		cin >> temp;
		y += temp;
	}

	if(x || y || z){
		cout << "NO";
	}else {
		cout << "YES";
	}

	return 0;
}